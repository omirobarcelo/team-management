{
  "version": 1,
  "projects": {
    "gymnastika": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/gymnastika",
      "sourceRoot": "apps/gymnastika/src",
      "prefix": "snk",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/gymnastika",
            "index": "apps/gymnastika/src/index.html",
            "main": "apps/gymnastika/src/main.ts",
            "polyfills": "apps/gymnastika/src/polyfills.ts",
            "tsConfig": "apps/gymnastika/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/gymnastika/src/favicon.ico",
              "apps/gymnastika/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": ["apps/gymnastika/src/styles.scss", "apps/gymnastika/src/theme.less"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/gymnastika/src/environments/environment.ts",
                  "with": "apps/gymnastika/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gymnastika:build",
            "proxyConfig": "apps/gymnastika/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "gymnastika:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gymnastika:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/gymnastika/tsconfig.app.json",
              "apps/gymnastika/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/gymnastika/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/gymnastika/jest.config.js",
            "tsConfig": "apps/gymnastika/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/gymnastika/src/test-setup.ts"
          }
        }
      }
    },
    "gymnastika-e2e": {
      "root": "apps/gymnastika-e2e",
      "sourceRoot": "apps/gymnastika-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gymnastika-e2e/cypress.json",
            "tsConfig": "apps/gymnastika-e2e/tsconfig.e2e.json",
            "devServerTarget": "gymnastika:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gymnastika:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/gymnastika-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/gymnastika-e2e/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "feature-shared-exercises": {
      "projectType": "library",
      "root": "libs/feature/shared/exercises",
      "sourceRoot": "libs/feature/shared/exercises/src",
      "prefix": "snk",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature/shared/exercises/tsconfig.lib.json",
              "libs/feature/shared/exercises/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/feature/shared/exercises/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/feature/shared/exercises/jest.config.js",
            "tsConfig": "libs/feature/shared/exercises/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/feature/shared/exercises/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "gymnastika"
}
